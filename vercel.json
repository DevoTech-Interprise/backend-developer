{
  "version": 2,
  "builds": [
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/register",
      "dest": "/api/register.ts"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/login.ts"
    },
    {
      "src": "/api/auth/profile",
      "dest": "/api/profile.ts"
    },
    {
      "src": "/api",
      "dest": "/api/index.ts"
    },
    {
      "src": "/",
      "dest": "/api/index.ts"
    }
  ]
}
