{
  "version": 2,
  "builds": [
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/register",
      "dest": "/api/register.ts"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/login.ts"
    },
    {
      "src": "/api/auth/profile",
      "dest": "/api/profile.ts"
    },
    {
      "src": "/api/auth/verify",
      "dest": "/api/verify.ts"
    },
    {
      "src": "/api/auth/refresh",
      "dest": "/api/refresh.ts"
    },
    {
      "src": "/api/users/me",
      "dest": "/api/users-me.ts"
    },
    {
      "src": "/api/users",
      "dest": "/api/users-get.ts"
    },
    {
      "src": "/api/users/(.*)/promote",
      "dest": "/api/users-promote.ts"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users-id-get.ts"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users-id-put.ts",
      "methods": ["PUT"]
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users-id-delete.ts",
      "methods": ["DELETE"]
    },
    {
      "src": "/api",
      "dest": "/api/index.ts"
    },
    {
      "src": "/",
      "dest": "/api/index.ts"
    }
  ]
}
